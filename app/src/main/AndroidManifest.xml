<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.umaaamm.ingetin">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ipb"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ipb"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">

        </activity>

        <activity
            android:name="com.example.umaaamm.ingetin.SplashScreen"
            android:theme="@style/SplassScreenCustom">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.umaaamm.ingetin.Login"
            android:label="Login Aplikasi"
            android:theme="@style/MasukDaftarnoActionbar">

        </activity>
        <service
            android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
    </application>

</manifest>